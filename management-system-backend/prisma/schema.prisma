
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = "postgres://postgres:root@localhost:5432/triaodo"
  directUrl = "postgres://postgres:root@localhost:5432/triaodo"
}

model Department {
  id                       Int             @id @default(autoincrement())
  name                     String
  description              String
  managing_department_id   Int?
  
}

